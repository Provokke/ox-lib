{
  "name": "ox_lib-retro-windows-theme",
  "version": "1.0.0",
  "description": "A nostalgic Windows 95/2000 themed reskin for ox_lib that brings back the classic retro computing aesthetic to FiveM servers",
  "main": "fxmanifest.lua",
  "keywords": [
    "fivem",
    "ox_lib",
    "theme",
    "retro",
    "windows95",
    "windows2000",
    "ui",
    "reskin",
    "nostalgia",
    "classic"
  ],
  "author": {
    "name": "Retro Theme Developer",
    "email": "developer@example.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ox_lib-retro-windows-theme.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/ox_lib-retro-windows-theme/issues"
  },
  "homepage": "https://github.com/yourusername/ox_lib-retro-windows-theme#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "build": "cd web && npm run build",
    "dev": "cd web && npm start",
    "install-deps": "cd web && npm install",
    "clean": "rm -rf web/build web/dist",
    "prepare": "npm run install-deps && npm run build"
  },
  "peerDependencies": {
    "ox_lib": ">=3.0.0"
  },
  "devDependencies": {},
  "files": [
    "web/src/theme/retro-windows.ts",
    "web/src/index.css",
    "web/src/components/",
    "fxmanifest.lua",
    "README-RETRO-THEME.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "fivem": {
    "resource": "ox_lib",
    "type": "theme",
    "compatibility": {
      "ox_lib": ">=3.0.0",
      "fivem": ">=6683"
    },
    "features": [
      "Windows 95/2000 UI styling",
      "Authentic retro color palette",
      "MS Sans Serif typography",
      "Complete component coverage",
      "Pixel-perfect design"
    ]
  },
  "theme": {
    "name": "Retro Windows",
    "version": "1.0.0",
    "style": "Windows 95/2000",
    "colors": {
      "primary": "#0080c0",
      "secondary": "#c0c0c0",
      "accent": "#004080",
      "background": "#c0c0c0",
      "text": "#000000"
    },
    "fonts": {
      "primary": "MS Sans Serif",
      "monospace": "Courier New"
    },
    "components": [
      "notifications",
      "progress-bars",
      "context-menus",
      "dialogs",
      "buttons",
      "inputs",
      "menus"
    ]
  },
  "installation": {
    "method": "replace",
    "backup_required": true,
    "build_required": true,
    "restart_required": true
  },
  "changelog": {
    "1.0.0": {
      "date": "2024-01-20",
      "changes": [
        "Initial release",
        "Complete Windows 95/2000 theme implementation",
        "Authentic retro styling for all components",
        "Comprehensive documentation"
      ]
    }
  }
}